{
  "name": "nodebeats",
  "version": "0.0.3",
  "private": true,
  "description": "node content management framework developed by BitsBeat IT Solution",
  "apidoc": {
    "name": "nodebeats",
    "title": "Nodebeats Api Docs",
    "url": "http://localhost:3000"
  },
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "NODE_ENV=test node server.js",
    "outdated": "npm outdated --depth 0",
    "coverage": "istanbul cover _mocha -- -R spec",
    "start": "NODE_ENV=development node server.js",
    "copy": "cp app-src/ app/",
    "build:client:prod": "NODE_ENV=production  npm run clean:client && webpack --config webpack-client.config.js -p",
    "build:admin:prod": "NODE_ENV=production  npm run clean:admin  && webpack -p",
    "build:client": " NODE_ENV=developement webpack --config webpack-client.config.js -d",
    "build:admin": "NODE_ENV=developement webpack -d",
    "build:sw": "gulp service-worker",
    "clean:admin": "gulp clean:admin",
    "clean:client": "gulp clean:client",
    "clean:uploads": "npm run clean:images && npm run clean:documents",
    "clean:images": "rm -rf ./public/uploads/images/*/",
    "clean:documents": "rm -rf ./public/uploads/documents/*/",
    "test:admin": "karma",
    "test:e2e": "protractor"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/BitsBeat/nodeframework.git"
  },
  "keywords": [
    "Node",
    "Express",
    "JWT",
    "MFA",
    "2FA"
